# https://editorconfig.org  --多人协同--不同IDE和编辑器统一代码规范和风格

root = true    #editorconfig顶级配置文件,停止向上寻找配置文件

[*]
end_of_line = lf    #换行符类型=lf(macos、linux)   windows为crlf，git默认提交为lf
charset = utf-8
indent_style = space
indent_size = 2
trim_trailing_whitespace = true    #删除行尾空格
insert_final_newline = true

[*.md]
trim_trailing_whitespace = false
